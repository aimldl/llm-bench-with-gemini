 "version": "0.2.0",
 "configurations": [
  
   {
     "name": "Run SEA-HELM with Vertex AI",
     "type": "debugpy",
     "request": "launch",
     "program": "seahelm_evaluation.py",
     "console": "integratedTerminal",
     "args": [
                "--tasks", "seahelm",
                "--model_type", "litellm",
                "--output_dir", "output-vertex_ai/gemini-2.5-flash",
                "--model_name", "gemini-2.5-flash",
                "--model_args", "api_provider=vertex_ai",
                "--skip_tokenize_prompts",
                "--rerun_cached_results --limit 1",
                "--skip_tasks multi_turn"
            ]
   }
 ]
}
